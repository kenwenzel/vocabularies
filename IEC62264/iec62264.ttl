# Storage Zone properties -----------------------------------
	  
iec62264:storageZoneId a owl:DatatypeProperty ;
      skos:prefLabel "storage Zone Id"@en ;
      skos:definition "Storage Zone Identification according to Schaeffler nomenclature"@en ;
      rdfs:domain iec62264:StorageZone ;	  
      rdfs:range xsd:string ;
	.  
	  
iec62264:storageZoneName a owl:DatatypeProperty ;
      skos:prefLabel "storage Zone Name"@en ;
      skos:definition "Name of the Storage Zone"@en ;
      rdfs:domain iec62264:StorageZone ;	  
